        "import numpy as np\n",
        "import random\n",
        "def recp(c):\n",
        "  out=np.empty(len(c))\n",
        "  for i in range(len(c)):\n",
        "    out[i]= 1.0/c[i]\n",
        "  return out  \n",
        "\n",
        "a=np.random.randint(1,10,5)\n",
        "print(a)\n",
        "print(recp(a))\n",
        "%timeit recp(a)\n",
        "b=np.random.randint(1,100,1000000)\n",
        "print(1.0/b)\n",
        "%timeit recp(b)\n",
        "%timeit (1.0/b)"
